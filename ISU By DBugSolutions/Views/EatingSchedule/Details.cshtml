@model EatingSchedule
@{
    ViewData["Title"] = "Eating Schedule";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<img class="mb-3" src="~/Images/meal=schedule-header.png" alt="Creche List" width="300" />
<hr />

<div>

    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CrecheName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CrecheName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.MealName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MealName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Date)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Date)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index"><img src="~/Images/back-icon.png" alt="back" style="width:35px;" class="mr-1" /></a>

</div>
